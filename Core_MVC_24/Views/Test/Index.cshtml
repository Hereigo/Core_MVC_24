<h4>Submit Changes</h4>

<div class="row">
    <div class="col-md-4">
        <form asp-action="TestSubmit">
            <div class="form-group">
                <label class="control-label"></label>
                <input type="password" class="form-control" id="blablatest" name="blablatest" />
            </div>
            <div class="form-group">
                <input type="submit"
                       value="@(ViewBag.fileExists ? "Finilize" : "Begin")"
                       class="btn @(ViewBag.fileExists ? "btn-danger" : "btn-primary")" />
            </div>
        </form>
    </div>
</div>
